{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "include": [
    "src",
    "tests"
  ],
  "exclude": [
    ".cursor",
    ".git",
    ".mypy_cache",
    ".pytest_cache",
    ".ruff_cache",
    "**/__pycache__",
    "blueprints",
    "docs",
    "docker",
    "models",
    "samples",
    "tmp",
    "tools"
  ],
  "executionEnvironments": [
    {
      "root": "./src",
      "extraPaths": [
        "./src"
      ]
    },
    {
      "root": "./tests",
      "extraPaths": [
        "./src"
      ]
    }
  ]
}

